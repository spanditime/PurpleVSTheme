{
	"name": "Shades of Purple",
	"type": "dark",
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"enumMember": {
			"foreground": "#6875b6"
		},
		"variable.constant": {
			"foreground": "#52cea8"
		},
		"variable.defaultLibrary": {
			"foreground": "#b94a7f"
		}
	},
	"tokenColors": [
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#6720bd"
			}
		},
		{
			"name": "String Escape",
			"scope": "constant.character.escape, text.html constant.character.entity.named, punctuation.definition.entity.html",
			"settings": {
				"foreground": "#851efa"
			}
		},
		{
			"name": "Boolean",
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#ac2fff"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "Identifier",
			"scope": "variable, support.variable, support.class, support.constant, meta.definition.variable, entity.name.function",
			"settings": {
				"foreground": "#c66aff"
			}
		},
		{
			"name": "Constant",
			"scope": "variable.other.constant",
			"settings": {
				"foreground": "#f351ff"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword.control.flow.js",
			"settings": {
				"foreground": "#9d00ff"
			}
		},
		{
			"name": "Function call",
			"scope": "entity.name.function, support.function",
			"settings": {
				"foreground": "#d400ff"
			}
		},
		{
			"name": "Storage",
			"scope": "storage.type, storage.modifier, keyword.operator.expression",
			"settings": {
				"foreground": "#862fff"
			}
		},
		{
			"name": "Type",
			"scope": "support.type",
			"settings": {
				"foreground": "#8000ff"
			}
		},
		{
			"name": "Type",
			"scope": "entity.name.type, entity.other.inherited-class",
			"settings": {
				"foreground": "#a200ff"
			}
		},
		{
			"name": "Class variable",
			"scope": "variable.object.property, meta.field.declaration entity.name.function",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "Class method",
			"scope": "meta.definition.method entity.name.function",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "Function definition",
			"scope": "meta.function, entity.name.function",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "Template expression",
			"scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
			"settings": {
				"foreground": "#822fff"
			}
		},
		{
			"name": "Reset embedded/template expression colors",
			"scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "YAML key",
			"scope": "entity.name.tag.yaml",
			"settings": {
				"foreground": "#b846ff"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#5e2c91"
			}
		},
		{
			"name": "JSON key",
			"scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
			"settings": {
				"foreground": "#7b5eee"
			}
		},
		{
			"name": "JSON constant",
			"scope": "constant.language.json",
			"settings": {
				"foreground": "#bd2fff"
			}
		},
		{
			"name": "CSS class",
			"scope": "entity.other.attribute-name.class",
			"settings": {
				"foreground": "#b32fff"
			}
		},
		{
			"name": "CSS ID",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#7d14a7"
			}
		},
		{
			"name": "CSS tag",
			"scope": "source.css entity.name.tag",
			"settings": {
				"foreground": "#c55eee"
			}
		},
		{
			"name": "HTML tag outer",
			"scope": "meta.tag, punctuation.definition.tag",
			"settings": {
				"foreground": "#57119d"
			}
		},
		{
			"name": "HTML tag inner",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#8d26d2"
			}
		},
		{
			"name": "HTML tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#bf00ff"
			}
		},
		{
			"name": "Markdown heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#b62fff"
			}
		},
		{
			"name": "Markdown link text",
			"scope": "text.html.markdown meta.link.inline, meta.link.reference",
			"settings": {
				"foreground": "#b246ff"
			}
		},
		{
			"name": "Markdown list item",
			"scope": "text.html.markdown beginning.punctuation.definition.list",
			"settings": {
				"foreground": "#9a2fff"
			}
		},
		{
			"name": "Markdown code block",
			"scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
			"settings": {
				"foreground": "#6214a7"
			}
		},
		{
			"name": "Markdown inline code",
			"scope": "markup.inline.raw.string.markdown",
			"settings": {
				"foreground": "#6214a7"
			}
		},
		{
			"name": "Markdown Punctuation",
			"scope": [
				"punctuation.definition.heading.markdown",
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown",
				"punctuation.definition.quote.begin.markdown",
				"punctuation.definition.list.begin.markdown",
				"markup.fenced_code.block.markdown",
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#6735a0"
			}
		},
		{
			"name": "INI property name",
			"scope": "keyword.other.definition.ini",
			"settings": {
				"foreground": "#ac46ff"
			}
		},
		{
			"name": "INI section title",
			"scope": "entity.name.section.group-title.ini",
			"settings": {
				"foreground": "#b32fff"
			}
		},
		{
			"name": "C# class method",
			"scope": "source.cs meta.method.identifier entity.name.function",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "C# function call",
			"scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
			"settings": {
				"foreground": "#bb00ff"
			}
		},
		{
			"name": "C# type",
			"scope": "source.cs storage.type",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "C# return type",
			"scope": "source.cs meta.method.return-type",
			"settings": {
				"foreground": "#be5eee"
			}
		},
		{
			"name": "C# preprocessor",
			"scope": "source.cs meta.preprocessor",
			"settings": {
				"foreground": "#3f1b51"
			}
		},
		{
			"name": "C# namespace",
			"scope": "source.cs entity.name.type.namespace",
			"settings": {
				"foreground": "#ffffff"
			}
		},
	],
